<T3DataStructure>
	<sheets>
		<filteroptions>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.filterOptions</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.toolList>
						<TCEforms>
							<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.tool</label>
							<config>
								<size>5</size>
								<maxitems>9999</maxitems>
								<minitems>0</minitems>
								<type>select</type>
								<foreign_table>tx_rkwtools_domain_model_tool</foreign_table>
								<foreign_table_where>
									AND tx_rkwtools_domain_model_tool.deleted = 0 AND tx_rkwtools_domain_model_tool.hidden = 0
								</foreign_table_where>
							</config>
						</TCEforms>
					</settings.toolList>
					<settings.projectsList>
						<TCEforms>
							<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.projects</label>
							<config>
								<size>5</size>
								<maxitems>5</maxitems>
								<minitems>0</minitems>
								<type>select</type>
								<foreign_table>tx_rkwprojects_domain_model_projects</foreign_table>
								<foreign_table_where>
									AND tx_rkwprojects_domain_model_projects.deleted = 0 AND tx_rkwprojects_domain_model_projects.hidden = 0 AND tx_rkwprojects_domain_model_projects.visibility = 1 ORDER BY tx_rkwprojects_domain_model_projects.short_name
								</foreign_table_where>
							</config>
						</TCEforms>
					</settings.projectsList>
					<settings.departmentList>
						<TCEforms>
							<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.department</label>
							<config>
								<size>5</size>
								<maxitems>5</maxitems>
								<minitems>0</minitems>
								<type>select</type>
								<foreign_table>tx_rkwbasics_domain_model_department</foreign_table>
								<foreign_table_where>
									AND tx_rkwbasics_domain_model_department.deleted = 0 AND tx_rkwbasics_domain_model_department.hidden = 0 AND tx_rkwbasics_domain_model_department.visibility = 1 ORDER BY tx_rkwbasics_domain_model_department.name
								</foreign_table_where>
							</config>
						</TCEforms>
					</settings.departmentList>
                    <settings.sysCategoryList>
                        <TCEforms>
                            <label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.sysCategory</label>
                            <config>
                                <autoSizeMax>50</autoSizeMax>
                                <maxitems>9999</maxitems>
                                <minitems>0</minitems>
                                <type>select</type>
                                <foreign_table>sys_category</foreign_table>
                                <foreign_table_where>
                                    AND sys_category.deleted = 0 AND sys_category.hidden = 0 ORDER BY sys_category.title
                                </foreign_table_where>
                                <renderMode>tree</renderMode>
                                <treeConfig>
                                    <appearance>
                                        <expandAll>0</expandAll>
                                        <showHeader>1</showHeader>
                                    </appearance>
                                    <parentField>parent</parentField>
                                </treeConfig>
                            </config>
                        </TCEforms>
                    </settings.sysCategoryList>
				</el>
			</ROOT>
		</filteroptions>


		<frontendoptions>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.frontendOptions</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.itemsPerPage>
						<TCEforms>
							<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.itemsPerPage</label>
							<config>
								<type>input</type>
								<size>20</size>
								<max>4</max>
								<eval>trim,int</eval>
							</config>
						</TCEforms>
					</settings.itemsPerPage>
					<settings.showMoreLink>
						<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.showMoreLink</label>
						<config>
							<type>check</type>
						</config>
					</settings.showMoreLink>
					<settings.openLinksInSameWindow>
						<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.openWindow</label>
						<config>
							<type>check</type>
						</config>
					</settings.openLinksInSameWindow>
				</el>
			</ROOT>
		</frontendoptions>



		<frontendfilteroptions>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.frontendFilterOptions</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.showDepartment>
						<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.showDepartment</label>
						<config>
							<type>check</type>
						</config>
					</settings.showDepartment>
					<settings.showCategory>
						<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.showSysCategory</label>
						<config>
							<type>check</type>
						</config>
					</settings.showCategory>
					<settings.showProjects>
						<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.showProjects</label>
						<config>
							<type>check</type>
						</config>
					</settings.showProjects>
				</el>
			</ROOT>
		</frontendfilteroptions>



		<misc>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.misc</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.noCache>
						<label>LLL:EXT:rkw_tools/Resources/Private/Language/locallang_db.xlf:flexform.overview.noCache</label>
						<config>
							<type>check</type>
						</config>
					</settings.noCache>
				</el>
			</ROOT>
		</misc>

		
	</sheets>
</T3DataStructure>